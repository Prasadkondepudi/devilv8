gg.searchNumber("1", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(100)
gg.editAll("0", gg.TYPE_DWORD)
gg.clearResults()
gg.toast(" cœÉ‚àÇ—î –≤y ‚àÇ—îvi‚Ñì –ΩŒ±c–∫s")
end

