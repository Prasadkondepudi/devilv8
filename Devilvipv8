function MH()
 MN2 = gg.multiChoice({
 	"‚ú≥Ô∏è  ü·¥áss  Ä·¥á·¥Ñ·¥è…™ ü  ü·¥è ô ô è",
     "‚ú≥Ô∏è ·¥Ä·¥ú·¥õ·¥è  ú·¥á·¥Ä·¥Ös ú·¥è·¥õs",
     "‚ú≥Ô∏è ·¥Ä…¥·¥õ·¥á…¥…¥·¥Ä",
     "‚ú≥Ô∏è  ü·¥áss  Ä·¥á·¥Ñ·¥è…™ ü …¢·¥Ä·¥ç·¥á",
     "‚ú≥Ô∏è  ú·¥á·¥Ä·¥Ös ú·¥è·¥õs …¢·¥Ä·¥ç·¥á",
     "‚ú≥Ô∏è  ô ü·¥Ä·¥Ñ·¥ã s·¥ã è",
     "‚ú≥Ô∏è  ú·¥ú ü·¥ã ·¥ä·¥ú·¥ç·¥ò …¥·¥è ·¥Ö·¥Ä·¥ç·¥Ä…¢·¥á",
     "‚ú≥Ô∏è ·¥ç·¥Ä…¢…™·¥Ñ  ô·¥ú ü ü·¥á·¥õ",
     "‚éã–≤Œ±c–∫",
}, nil, " ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ·¥ç·¥á…¥·¥ú  ú·¥Ä·¥Ñ·¥ãs‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ")
if MN2 == nil then
else
if MN2[1] == true then M1()end
if MN2[2] == true then M2()end
if MN2[3] == true then M3()end
if MN2[4] == true then M4()end
if MN2[5] == true then M5()end
if MN2[6] == true then M6()end
if MN2[7] == true then M7()end
if MN2[8] == true then M8()end
if MN2[9] == true then HOME()end
end
PUBGMH = -1
end

function M1()
