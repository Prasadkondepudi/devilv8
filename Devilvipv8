function HOME()
 MN = gg.choice({
 "‚î£‚û£ ·¥°·¥Ä ü ü  ú·¥Ä·¥Ñ·¥ã & ·¥Ñ·¥è ü·¥è·¥ú Ä",
 "‚î£‚û£ ·¥ç·¥á…¥·¥ú ú·¥Ä·¥Ñ·¥ãs",
 "‚î£‚û£ ·¥Ñ ü·¥á·¥Ä Ä  ü·¥è…¢s ·¥Ä ü ü ·¥ò·¥ú ô…¢",
 "‚î£‚û£ „Äê ·¥áx…™·¥õ „Äë",
 },nil, (os.date("‚î£‚û£ùêÉùêÑùêïùêàùêã¬†ùêïùêàùêè¬†ùêíùêÇùêëùêàùêèùêì¬†ùêïùü¥\n‚î£‚û£s·¥Ñ Ä…™·¥ò·¥õ  ô è :·¥Ö·¥á·¥†…™ ü\n‚î£‚û£·¥õ·¥è·¥Ö·¥Ä è: %Y/%m/%d  ·¥õ…™·¥ç·¥á: %H:%M:%S")))
if MN == nil then
else
if MN == 1 then WC()end
if MN == 2 then MH()end
if MN == 3 then CLEARLOGS()end
if MN == 4 then EXIT()end
end
PUBGMH = -1
end

